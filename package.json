{
  "name": "e-commerce-app-react",
  "version": "1.0.0",
  "description": "MERN App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \" npm run client\"",
    "start:import": "node backend/seeder",
    "start:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubaw00/E-commerce-react-app.git"
  },
  "author": "Jakub Widurek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kubaw00/E-commerce-react-app/issues"
  },
  "homepage": "https://github.com/kubaw00/E-commerce-react-app#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/colors": "^1.2.1",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.2",
    "colors": "^1.4.0",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.0.3"
  }
}
